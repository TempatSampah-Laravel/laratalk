{
    "name": "febrihidayan/laratalk",
    "description": "Laratalk provides Laravel based chat between users and groups",
    "keywords": ["laratalk", "packages", "laravel", "chatting", "realtime", "messages", "pusher"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/febrihidayan/laratalk/issues",
        "source": "https://github.com/febrihidayan/laratalk"
    },
    "authors": [
        {
            "name": "Febri Hidayan",
            "email": "febrihidayan20@gmail.com"
        }
    ],
    "type": "library",
    "require": {
        "php": "^7.2|^8.0",
        "laravel/framework": "^6.0|^7.0|^8.0",
        "pusher/pusher-php-server": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Laratalk\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laratalk\\LaratalkServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
